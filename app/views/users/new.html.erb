<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>
<%= form_with(model: @user, url: users_path) do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :email %>
  <%= form.text_field :email %>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <%= form.select :type, [["none",""], ["admin","Admin"]] %>

  <%= form.submit %>
<% end %>