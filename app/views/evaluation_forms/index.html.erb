<h1>EvaluationForms#index</h1>
<p>Find me in app/views/evaluation_forms/index.html.erb</p>
  
<ul>
  <% @evaluation_forms.each do |evaluation_form| %>
    <li>
      <%= link_to evaluation_form.id, evaluation_form_path(evaluation_form) %>&nbsp;&nbsp;&nbsp;
    <%= link_to "Edit evaluation_form", edit_evaluation_form_path(evaluation_form) %>&nbsp;&nbsp;&nbsp;
    <%= link_to 'Delete', evaluation_form_path(evaluation_form), method: :delete, data: {
                    turbo_method: :delete,
                    turbo_confirm: "Are you sure?"
                  } %> 
</li>
    
  <% end %>
</ul>
<%= link_to "New evaluation_form", new_evaluation_form_path %>
